#include<stdio.h>
void printarray(int* arr, int n)
{
    printf("{");
    for(int i=0; i<n-1; i++)
    {
        printf("%d, ",arr[i]);
    }
    printf("%d}",arr[n-1]);
}
int linearsearch(int *arr, int n, int number)
{
    for(int i=0; i<n; i++)
    {
        if(number==arr[i])
        {
            return i;
        }
    }
}
void main()
{
    int arr[10];
    int n=sizeof(arr)/sizeof(arr[0]);
    int number;
    printf("Enter the array:\n");
    for(int i=0; i<n; i++)
    {
        printf("Enter arr[%d]:",i);
        scanf("%d",&arr[i]);
    }
    printarray(arr,n);
    printf("\nEnter the number you want to find:");
    scanf("%d",&number);
    int index=linearsearch(arr,n,number);
    if(index<3)
    {
        switch(index)
    {
    case 0:
        printf("It is the 1st element");
        break;

    case 1:
        printf("It is the 2nd element");
        break;

    case 2:
        printf("It is the 3rd element");
        break;

    default:
        break;
        }
    }
    else
    {
        printf("It is the %dth element",index+1);
    }
}
